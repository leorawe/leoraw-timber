{% extends "base-blog.twig" %}

{% block content %}
  <h1>{{wp_title}}</h1>
  <div class="content-wrapper">
    {% for post in posts %}
      {% include ['tease-'~post.post_type~'.twig', 'tease-blog.twig'] %}
    {% endfor %}

    {% include 'partial/pagination.twig' with { pagination: pagination } %}
  </div>
{% endblock %}
